    "vue": "^3.3.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.7",
    "@iconify-json/ep": "^1.1.10",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/js-cookie": "^3.0.3",
